{
  "name": "ascii2mathml",
  "version": "0.7.1",
  "description": "The second AsciiMath – MathML converter",
  "keywords": [
    "mathml",
    "asciimath",
    "asciimathml"
  ],
  "homepage": "https://runarberg.github.io/ascii2mathml/",
  "bugs": "https://github.com/runarberg/ascii2mathml/issues",
  "license": "MIT",
  "author": "Rúnar Berg Baugsson Sigríðarson <runarberg@zoho.com>",
  "main": "target/node/ascii2mathml.js",
  "bin": {
    "ascii2mathml": "bin/ascii2mathml.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/runarberg/ascii2mathml.git"
  },
  "scripts": {
    "clean": "rm -fr .nyc_output coverage/ target/",
    "lint": "eslint src/ test/",
    "build": "rollup --config",
    "prepare": "npm run build",
    "test": "nyc ava",
    "test:watch": "ava --watch"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": false
    },
    "parserOptions": {
      "ecmaVersion": "2019",
      "sourceType": "module"
    },
    "extends": [
      "airbnb-base",
      "plugin:ava/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "import/extensions": [
        "error",
        "ignorePackages"
      ],
      "no-use-before-define": [
        "error",
        {
          "functions": false
        }
      ]
    }
  },
  "nyc": {
    "reporter": [
      "html",
      "text",
      "text-summary"
    ]
  },
  "prettier": {
    "trailingComma": "all"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "ava": "^2.4.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-ava": "^9.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "expect.js": "^0.3.1",
    "nyc": "^14.1.1",
    "prettier": "1.19.1",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3"
  },
  "dependencies": {
    "minimist": "^1.2.0"
  }
}
